rule "Rule: generate vm.memory.util(Linux)"
	agenda-group "populate"
	activation-group "vm.memory.util"
	salience -25
when
	$m : Metric ( id == 'vm.memory.util')
	$m_free: Metric ( id == 'vm.memory.free')
	$m_cached: Metric ( id == 'vm.memory.cached')
	$m_buffers: Metric ( id == 'vm.memory.buffers')
	$m_total: Metric ( id == 'vm.memory.total')
then
    logger.debug(marker,"There are vm.memory.total and vm.memory.free,vm.memory.cached,vm.memory.buffers found: " + $m.getId());
    modify( $m ) { 
		setExpressionFormula( "((last(\042"+$m_total.getKeyQuotesEscaped()+"\042)-last(\042"+$m_free.getKeyQuotesEscaped()+"\042)-last(\042"+$m_cached.getKeyQuotesEscaped()+"\042)-last(\042"+$m_buffers.getKeyQuotesEscaped()+"\042))/last(\042"+$m_total.getKeyQuotesEscaped()+"\042))*100" )
	};
end

