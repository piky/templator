package org.zabbix.template.generator.objects;



rule "Rule 4: generate vm.memory.util"
	agenda-group "populate"
	activation-group "vm.memory.util"
	salience -50
when
	$m : Metric ( id == 'vm.memory.util')
	$m_used: Metric ( id == 'vm.memory.used')
	$m_total: Metric ( id == 'vm.memory.total')
then
    logger.debug(marker,"There are vm.memory.total and vm.memory.used found "+$m.getId());
    modify( $m ) { 
		setExpressionFormula( "(last(\042"+$m_used.getKeyQuotesEscaped()+"\042)/last(\042"+$m_total.getKeyQuotesEscaped()+"\042))*100" )
	};
end


