  <xs:complexType name="itemProtoObj">
	  <xs:complexContent>
	    <xs:extension base="itemObj">
	      <xs:sequence>
				<xs:element name="application_prototypes">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="application_prototype" maxOccurs="unbounded" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element
											type="xs:string" name="name" />
									</xs:sequence>
								</xs:complexType>
