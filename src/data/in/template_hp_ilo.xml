              </macro>
              <macro>
                  <macro>{$DISK_ARRAY_WARN_STATUS}</macro>
                  <value>3</value>
              </macro>
              
              <macro>
                  <macro>{$DISK_OK_STATUS}</macro>
                  <value>2</value>
              </macro>
              <macro>
                  <macro>{$DISK_WARN_STATUS}</macro>
                  <value>4</value>
              </macro>
              <macro>
                  <macro>{$DISK_FAIL_STATUS}</macro>
                  <value>3</value>
              </macro>
            </macros>
  	</template>
  
  
