<?xml version="1.0" encoding="UTF-8"?>
<tns:root xmlns:tns="http://www.example.org/zbx_template_new/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.example.org/zbx_template_new/ ../../main/resources/templates/metrics.xsd ">
  <template>
  	  <classes>
  	  	<class>Fault</class>
  	  	<class>Inventory</class>
  	  </classes>
	  <name>Template HP iLO</name>
	  <description>Template HP iLO</description>
	  <sysObjectID>.1.3.6.1.4.1.232.9.4.10</sysObjectID>
	  
	  <discoveryRules>
	  	<discoveryRule>
			<name>Temperature Discovery</name>
                <snmp_oid>discovery[{#SNMPVALUE},.1.3.6.1.4.1.232.6.2.6.8.1.1,{#SENSOR_LOCALE},.1.3.6.1.4.1.232.6.2.6.8.1.3]</snmp_oid>
                <key>tempDescr.discovery</key>
                <description>Scanning table of Temperature Sensor Entries: CPQHLTH-MIB::cpqHeTemperatureTable</description>
				<filter>
                  <evaltype>0</evaltype>
                  <formula/>
                  <conditions>
                      <condition>
                          <macro>{#SENSOR_LOCALE}</macro>
                          <value>(3|4|5|7|8|9|10|12|13)</value><!-- ambient -->
                          <operator>8</operator>
                          <formulaid>A</formulaid>
                      </condition>
                  </conditions>
        	    </filter>
		</discoveryRule>
	    <discoveryRule>
			<name>Temperature Discovery Ambient</name>
            <snmp_oid>discovery[{#SNMPVALUE},.1.3.6.1.4.1.232.6.2.6.8.1.1,{#SENSOR_LOCALE},.1.3.6.1.4.1.232.6.2.6.8.1.3]</snmp_oid>
            <key>tempDescr.discovery.ambient</key>
            <description>Scanning table of Temperature Sensor Entries: CPQHLTH-MIB::cpqHeTemperatureTable with ambient(11) and 0.1 index filter</description>
        	<filter>
                  <evaltype>0</evaltype>
                  <formula/>
                  <conditions>
                      <condition>
                          <macro>{#SNMPINDEX}</macro>
                          <value>0\.1</value> <!-- ambient intake index-->
                          <operator>8</operator>
                          <formulaid>B</formulaid>
                      </condition>
                      <condition>
                          <macro>{#SENSOR_LOCALE}</macro>
                          <value>11</value><!-- ambient -->
                          <operator>8</operator>
                          <formulaid>A</formulaid>
                      </condition>
                  </conditions>
            </filter>
		</discoveryRule>
		
	    <discoveryRule>
			<name>Temperature Discovery CPU</name>
            <snmp_oid>discovery[{#SNMPVALUE},.1.3.6.1.4.1.232.6.2.6.8.1.1,{#SENSOR_LOCALE},.1.3.6.1.4.1.232.6.2.6.8.1.3]</snmp_oid>
            <key>tempDescr.discovery.cpu</key>
            <description>Scanning table of Temperature Sensor Entries: CPQHLTH-MIB::cpqHeTemperatureTable with cpu(6) filter</description>
        	<filter>
                  <evaltype>0</evaltype>
                  <formula/>
                  <conditions>
                      <condition>
                          <macro>{#SENSOR_LOCALE}</macro>
                          <value>6</value><!-- cpu -->
                          <operator>8</operator>
                          <formulaid>A</formulaid>
                      </condition>
                  </conditions>
            </filter>
		</discoveryRule>
		<discoveryRule>
			<name>Array Controller Discovery</name>
            <snmp_oid>discovery[{#SNMPVALUE},.1.3.6.1.4.1.232.3.2.2.1.1.1,{#CNTLR_SLOT},1.3.6.1.4.1.232.3.2.2.1.1.5,{#CNTLR_LOCATION},1.3.6.1.4.1.232.3.2.2.1.1.20]</snmp_oid>
            <key>array.discovery</key>
            <description>Scanning table of Array controllers: CPQIDA-MIB::cpqDaCntlrTable </description>
		</discoveryRule>
		
		<discoveryRule>
			<name>Physical Disk Discovery</name>
            <snmp_oid>discovery[{#SNMPVALUE},1.3.6.1.4.1.232.3.2.5.1.1.5,{#DISK_LOCATION},1.3.6.1.4.1.232.3.2.5.1.1.64]</snmp_oid>
            <key>physicalDisk.discovery</key>
            <description>Scanning  table of physical drive entries CPQIDA-MIB::cpqDaPhyDrvTable.</description>
		</discoveryRule>
		
	  </discoveryRules>
	  <metrics>
	  	<temperatureValue>
	    	  <oid>.1.3.6.1.4.1.232.6.2.6.8.1.4.{#SNMPINDEX}</oid>
		      <snmpObject>cpqHeTemperatureCelsius[{#SNMPINDEX}]</snmpObject>
		      <mib>CPQHLTH-MIB</mib>
		      <discoveryRule>Temperature Discovery</discoveryRule>
		      <metricLocation>{#SNMPINDEX}</metricLocation>
	    </temperatureValue>
	  	<temperatureValue>
	    	  <oid>.1.3.6.1.4.1.232.6.2.6.8.1.4.{#SNMPINDEX}</oid>
		      <snmpObject>cpqHeTemperatureCelsius[Ambient-{#SNMPINDEX}]</snmpObject>
		      <mib>CPQHLTH-MIB</mib>
		      <discoveryRule>Temperature Discovery Ambient</discoveryRule>
		      <metricLocation>Ambient</metricLocation>
	    </temperatureValue>
	    <temperatureValue>
	    	  <oid>.1.3.6.1.4.1.232.6.2.6.8.1.4.{#SNMPINDEX}</oid>
		      <snmpObject>cpqHeTemperatureCelsius[CPU-{#SNMPINDEX}]</snmpObject>
		      <mib>CPQHLTH-MIB</mib>
		      <discoveryRule>Temperature Discovery CPU</discoveryRule>
		      <metricLocation>CPU</metricLocation>
	    </temperatureValue>
	    <!-- by element status 	    
	    <temperatureStatus>
	    	  <oid>.1.3.6.1.4.1.232.6.2.6.8.1.6.{#SNMPINDEX}</oid>
		      <snmpObject>cpqHeTemperatureCondition.[{#SNMPINDEX}]</snmpObject>
		      <mib>CPQHLTH-MIB</mib>
		      <discoveryRule>Temperature Discovery</discoveryRule>
		      <vendorDescription>The Temperature sensor condition.
		      This value will be one of the following:
		      other(1)  Temperature could not be determined.
		      ok(2)  The temperature sensor is within normal operating range.
		      degraded(3)  The temperature sensor is outside of normal operating range.
		      failed(4)  The temperature sensor detects a condition that could  permanently damage the system.
		      The system will automatically shutdown if the failed(4) condition results,
		      so it is unlikely that this value will ever be returned by the agent.
		      If the cpqHeThermalDegradedAction is set to shutdown(3) the system will be shutdown if the degraded(3) condition occurs.
		      </vendorDescription>
		      <metricLocation>{#SNMPINDEX}</metricLocation>
		      <valueMap>CPQSINFO-MIB::status</valueMap>
	    </temperatureStatus>-->
	  	<temperatureStatus>
	    	  <oid>1.3.6.1.4.1.232.6.2.6.1.0</oid>
		      <snmpObject>cpqHeThermalCondition.0</snmpObject>
		      <mib>CPQHLTH-MIB</mib>
		      <vendorDescription>This value specifies the overall condition of the system's
		      thermal environment.
		      This value will be one of the following:
		      other(1)  Temperature could not be determined.
		      ok(2)  The temperature sensor is within normal operating range.
		      degraded(3)  The temperature sensor is outside of normal operating range.
		      failed(4)  The temperature sensor detects a condition that could  permanently damage the system.
		      </vendorDescription>
		      <metricLocation>System</metricLocation>
		      <valueMap>CPQSINFO-MIB::status</valueMap>
	    </temperatureStatus>
	  	<temperatureLocale>
	    	  <oid>.1.3.6.1.4.1.232.6.2.6.8.1.3.{#SNMPINDEX}</oid>
		      <snmpObject>cpqHeTemperatureLocale[{#SNMPINDEX}]</snmpObject>
		      <mib>CPQHLTH-MIB</mib>
		      <discoveryRule>Temperature Discovery</discoveryRule>
		      <vendorDescription>This specifies the location of the temperature sensor present in the system.		      </vendorDescription>
		      <metricLocation>{#SNMPINDEX}</metricLocation>
		      <valueMap>CPQHLTH-MIB::cpqHeTemperatureLocale</valueMap>
	    </temperatureLocale>
	  	
	    <overallStatus>
	      <oid>.1.3.6.1.4.1.232.2.1.3.0</oid>
	      <snmpObject>cpqSiMibCondition.0</snmpObject>
	      <mib>CPQSINFO-MIB</mib>
	      <vendorDescription>The overall condition. This object represents the overall status of the server information represented by this MIB.</vendorDescription>
	      <valueMap>CPQSINFO-MIB::status</valueMap>
	    </overallStatus>
	      

	    <diskArrayStatus>
	      <oid>1.3.6.1.4.1.232.3.2.2.1.1.6.{#SNMPINDEX}</oid>
	      <snmpObject>cpqDaCntlrCondition[{#SNMPINDEX}]</snmpObject>
	      <mib>CPQIDA-MIB</mib>
	      <discoveryRule>Array Controller Discovery</discoveryRule>
	      <vendorDescription>This value represents the overall condition of this controller,
	      and any associated logical drives,physical drives, and array accelerators.</vendorDescription>
	      <!-- <metricLocation>{#CNTLR_SLOT}</metricLocation>  -->
	      <metricLocation>{#CNTLR_LOCATION}</metricLocation>
	      <valueMap>CPQSINFO-MIB::status</valueMap>
	    </diskArrayStatus>
	    
	    <diskArrayModel>
	      <oid>1.3.6.1.4.1.232.3.2.2.1.1.2.{#SNMPINDEX}</oid>
	      <snmpObject>cpqDaCntlrModel[{#SNMPINDEX}]</snmpObject>
	      <mib>CPQIDA-MIB</mib>
	      <discoveryRule>Array Controller Discovery</discoveryRule>
	      <vendorDescription>Array Controller Model. The type of controller card.</vendorDescription>
	      <!-- <metricLocation>{#CNTLR_SLOT}</metricLocation>  -->
	      <metricLocation>{#CNTLR_LOCATION}</metricLocation>
	      <valueMap>CPQIDA-MIB::cpqDaCntlrModel</valueMap>
	    </diskArrayModel>
	    
	    <physicalDiskStatus>
	      <oid>1.3.6.1.4.1.232.3.2.5.1.1.6.{#SNMPINDEX}</oid>
	      <snmpObject>cpqDaPhyDrvStatus[{#SNMPINDEX}]</snmpObject>
	      <mib>CPQIDA-MIB</mib>
	      <discoveryRule>Physical Disk Discovery</discoveryRule>
	      <vendorDescription>Physical Drive Status. This shows the status of the physical drive. The following values are valid for the physical drive status:
	      other (1)  Indicates that the instrument agent does not recognize  the drive. 
	      You may need to upgrade your instrument agent  and/or driver software.
	      ok (2)  Indicates the drive is functioning properly.
	      failed (3)  Indicates that the drive is no longer operating and  should be replaced.
	      predictiveFailure(4)  Indicates that the drive has a predictive failure error and  should be replaced.
		  </vendorDescription>
	      <metricLocation>{#DISK_LOCATION}</metricLocation>
	      <valueMap>CPQIDA-MIB::cpqDaPhyDrvStatus</valueMap>
	    </physicalDiskStatus>
	    
	     <physicalDiskSerialNumber>
	      <oid>1.3.6.1.4.1.232.3.2.5.1.1.51.{#SNMPINDEX}</oid>
	      <snmpObject>cpqDaPhyDrvSerialNum[{#SNMPINDEX}]</snmpObject>
	      <mib>CPQIDA-MIB</mib>
	      <discoveryRule>Physical Disk Discovery</discoveryRule>
	      <vendorDescription>Physical Drive Serial Number.
	      This is the serial number assigned to the physical drive.
	      This value is based upon the serial number as returned by the SCSI inquiry command
	      but may have been modified due to spacelimitations.  This can be used for identification purposes.
		  </vendorDescription>
	      <metricLocation>{#DISK_LOCATION}</metricLocation>
	    </physicalDiskSerialNumber>

		<!-- inventory -->
	    <hwModel>
	       <oid>.1.3.6.1.4.1.232.2.2.4.2.0</oid>
	       <snmpObject>cpqSiProductName</snmpObject>
   	      <mib>CPQSINFO-MIB</mib>
   	      <vendorDescription>The machine product name.The name of the machine used in this system.</vendorDescription>
	    </hwModel>
	    
	    <hwSerialNumber>
	       <oid>.1.3.6.1.4.1.232.2.2.2.1.0</oid>
	       <snmpObject>cpqSiSysSerialNum</snmpObject>
   	      <mib>CPQSINFO-MIB</mib>
   	      <vendorDescription>The serial number of the physical system unit. The string will be empty if the system does not report the serial number function.</vendorDescription>
	    </hwSerialNumber>
	    
	   </metrics>

	   
	    <macros>
              <macro>
                  <macro>{$HEALTH_CRIT_STATUS}</macro>
                  <value>4</value>
              </macro>
              <macro>
                  <macro>{$HEALTH_WARN_STATUS}</macro>
                  <value>3</value>
              </macro>
              
			<macro>
                  <macro>{$TEMP_CRIT:"Ambient"}</macro>
